CREATE TABLE SHOPPING (ID INTEGER PRIMARY KEY, name TEXT, amount INTEGER);

DROP TABLE SHOPPING

ALTER TABLE SHOPPING RENAME TO SHOPP

ALTER TABLE SHOPP RENAME TO SHOPPING

